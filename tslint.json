{
  "extends": "tslint:recommended",
  "rules": {
    "no-consecutive-blank-lines": false,
    "quotemark": [true, "single", "avoid-template", "jsx-double"],
    "member-access": true,
    "no-any": true,
    "object-literal-sort-keys": {
      "options": [true, "match-declaration-order-only"]
    }
  }
}
